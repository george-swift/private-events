<h1>Create an Event</h1>

<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-default">
      <div class="panel-body">
        <%= render 'form', event: @event %>
      </div>
      <div class="panel-footer">
        <p><%= link_to 'Return to all events', root_path %></p>
      </div>
    </div>
  </div>
</div>
