<h1>Log in</h1> 
<div class="row">
<div class="col-md-6 col-md-offset-3"> 
    <%= form_with(url: login_path, scope: :session, local: true) do |f| %> 
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %><br>
      <%= f.submit "Log in", class: "btn btn-primary" %> 
    <% end %> 
  </div> 
</div> 
